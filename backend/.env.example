# Environment Profile ("local" or "production")
# Controls storage provider (S3 vs GCS), workflow execution (Docker vs K8s), CORS origins, etc.
ENVIRONMENT=local

# API Settings
APP_NAME="Corpus Service"
API_VERSION="v1"
DEBUG=true

# Database Components
DB_USER=postgres
DB_PASSWORD=password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=corpus

# AWS/S3
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_REGION=us-east-1
S3_BUCKET_NAME=corpus-bucket
S3_ENDPOINT_URL=http://localhost:4566

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/


REDIS_HOST="localhost"
REDIS_PORT=6379
REDIS_DB=0

# OpenRouter (unified AI provider)
OPENROUTER_API_KEY=sk-or-v1-xxx
DEFAULT_MODEL=google/gemini-2.0-flash-001

# Anthropic (for workflow agent execution)
ANTHROPIC_API_KEY=sk-ant-xxx

# OpenAI (for embeddings)
OPENAI_API_KEYS='["sk-xxx"]'

# Voyage AI (optional embedding provider)
VOYAGE_API_KEYS='["pa-xxx"]'

# Otel
# Service name and version (override as needed)
OTEL_SERVICE_NAME=corpus-service
OTEL_SERVICE_VERSION=1.0.0
AXIOM_TOKEN=token
AXIOM_DATASET=dataset


VECTORIZE_ORGANIZATION_ID=org
VECTORIZE_API_KEY=key


DATALAB_API_KEY=key


GOOGLE_PROJECT_ID=project
GOOGLE_REGION=region
GOOGLE_APPLICATION_CREDENTIALS=./path/to/creds.json
GEMINI_MODEL=gemini-2.0-flash-001
GEMINI_API_KEYS='["key1", "key2", "key3", "key4", "key5"]'

TEMPORAL_HOST="localhost:7233"
TEMPORAL_TASK_QUEUE=queue

PDF_PAGE_SPLIT_SIZE=1

# Firebase Auth (default SSO provider)
# Uses GOOGLE_APPLICATION_CREDENTIALS for local dev (already configured above)
# On GKE with Workload Identity, no credentials needed - ADC is used automatically
# Falls back to GOOGLE_PROJECT_ID if not set
FIREBASE_PROJECT_ID=your-firebase-project-id

# Auth0 SSO (optional - not used by default)
#AUTH0_DOMAIN=your-auth0-domain.auth0.com
#AUTH0_CLIENT_ID=your-auth0-client-id
#AUTH0_AUDIENCE=your-auth0-api-identifier

# Okta SSO (optional)
#OKTA_ISSUER=test
#OKTA_CLIENT_ID=test
#OKTA_AUDIENCE=test

QA_WORKER_PREFETCH_COUNT=5

# K8s/GCP settings (only used when ENVIRONMENT != local)
GCP_PROJECT_ID=your-gcp-project-id
WORKFLOW_AGENT_IMAGE_TAG=latest
EXA_API_KEY=test

# Billing - Stripe (payments)
STRIPE_SECRET_KEY=
STRIPE_PUBLISHABLE_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_ID_STARTER=
STRIPE_PRICE_ID_PROFESSIONAL=
STRIPE_PRICE_ID_BUSINESS=
STRIPE_PRICE_ID_ENTERPRISE=
